<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Set Accent color manually-->
    <Color x:Key="SystemAccentColor">#314a9b</Color>

    <Color x:Key="ColorPrimary">#263873</Color>
    <Color x:Key="ColorPrimaryDark">#1a264c</Color>

    <Color x:Key="ColorAppBar">#111632</Color>

    <Color x:Key="AppForegroundColorPrimary">#FF2B343D</Color>
    <Color x:Key="AppForegroundColorSecondary">#5A6570</Color>

    <Color x:Key="ListEvenColor">White</Color>
    <Color x:Key="ListOddColor">#FFF2F2F2</Color>
</ResourceDictionary>